<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="net.prabowoaz.consumerfavorite">

    <uses-permission android:name="android.permission.INTERNET"/>
    <permission
        android:name="net.prabowoaz.consumerfavorite.READ_DATABASE"
        android:protectionLevel="normal" />
    <permission
        android:name="net.prabowoaz.consumerfavorite.WRITE_DATABASE"
        android:protectionLevel="normal" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon_consumer"
        android:label="@string/app_name"
        android:roundIcon="@drawable/icon_consumer"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>